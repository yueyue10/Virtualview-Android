<VHLayout action="action"
    background="#FFFFFF"
    flag="flag_exposure|flag_clickable"
    layoutHeight="wrap_content"
    layoutWidth="85"
    orientation="V"
    xmlns="http://schemas.vv.com/apk/res/vv">

    <NImage layoutGravity="h_center"
        layoutHeight="25"
        layoutMarginTop="10"
        layoutWidth="50"
        src="https://gw.alicdn.com/tfs/TB1IoTCSpXXXXajXFXXXXXXXXXX-100-50.png"
        visibility="visible" />
    <FrameLayout layoutGravity="h_center"
        layoutHeight="wrap_content"
        layoutMarginBottom="10"
        layoutMarginTop="10"
        layoutWidth="wrap_content">
        <NImage layoutGravity="h_center"
            layoutHeight="65"
            layoutWidth="65"
            paddingBottom="@{${hasImgPadding} ? 10 : 0 }"
            paddingLeft="@{${hasImgPadding} ? 10 : 0 }"
            paddingRight="@{${hasImgPadding} ? 10 : 0 }"
            paddingTop="@{${hasImgPadding} ? 10 : 0 }"
            src="https://gw.alicdn.com/tfs/TB1Yf2iSpXXXXcmaXXXXXXXXXXX-130-130.png" />
        <NImage layoutGravity="right|top"
            layoutHeight="38"
            layoutMarginRight="-7"
            layoutWidth="38"
            src="https://gw.alicdn.com/tfs/TB1Zru_SpXXXXcaapXXXXXXXXXX-76-76.png"
            visibility="@{${tagUrl} ? visible : gone }" />
    </FrameLayout>

</VHLayout>